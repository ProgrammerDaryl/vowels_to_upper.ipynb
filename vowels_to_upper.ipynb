    "def vowelsToUpper(words):\n",
    "    # list the vowels\n",
    "    vowels = \"aeiou\"\n",
    "    # create an empty list for the output\n",
    "    uppercased_vowels = []\n",
    "    for letter in words:\n",
    "        if letter.lower() in vowels:\n",
    "            uppercased_vowels += letter.upper\n"
   ]
